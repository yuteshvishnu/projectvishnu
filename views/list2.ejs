</head>

<body>


    <% newitems.forEach(function(cartitem){  %>

    <img class="image" src="<%=cartitem.image%>" alt="">
    <h1><%=cartitem.name%></h1>
    <h3>
      <i class="fas fa-rupee-sign fa-1x"><%=cartitem.cost%></i>
    </h3>
    <br>
    Quantity:<input type="number" name="" value="<%=cartitem.count%>">
    <% }) %>


  <script src="app.js" charset="utf-8"></script>
</body>


</html>
